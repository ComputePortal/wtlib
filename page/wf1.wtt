import [core] from="./core.wtt"

// a centered version of wf0 in case of wide screens
export template wf1(title=null, description=null) extends=core super(
  $title, 
  $description,
  {
    position: relative,
    margin-left: auto,
    margin-right: auto,
    max-width: 1400px,
    height: 100%,
  }
)
